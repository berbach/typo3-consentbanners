{
  "name": "consent_banner",
  "version": "1.0.0",
  "description": "Consent Banner style",
  "repository": {
    "type": "git",
    "url": "https://github.com/npm/cli.git"
  },
  "license": "(MIT)",
  "main": "Base.js",
  "scripts": {
    "build": "npm run style && npm run script",
    "build:dev": "npm run style:dev & npm run script:dev",
    "script:dev": "webpack --mode=development -w --config script.webpack.js",
    "style:dev": "webpack --mode=development -w --config style.webpack.js",
    "script": "webpack --mode=production --config script.webpack.js",
    "style": "webpack --mode=production --config style.webpack.js"
  },
  "dependencies": {
    "bezier-easing": "^2.1.0",
    "core-js": "^3.16.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/register": "^7.13.16",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cssnano": "^5.0.5",
    "fast-glob": "^3.2.5",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.6.0",
    "path": "^0.12.7",
    "postcss": "^8.3.0",
    "postcss-loader": "^4.3.0",
    "sass": "^1.37.5",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.3",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.7.0"
  }
}
