	  "@babel/register": "^7.22.5",
	  "autoprefixer": "^10.4.15",
	  "babel-loader": "^9.1.3",
	  "clean-css": "^5.3.2",
	  "clean-webpack-plugin": "^4.0.0",
	  "css-loader": "^6.8.1",
	  "css-minimizer-webpack-plugin": "^5.0.1",
	  "cssnano": "^6.0.1",
	  "fast-glob": "^3.3.1",
	  "file-loader": "^6.2.0",
	  "mini-css-extract-plugin": "^2.7.6",
	  "path": "^0.12.7",
	  "postcss": "^8.4.29",
	  "postcss-loader": "^4.3.0",
	  "postinstall": "^0.8.0",
	  "resolve-url-loader": "^5.0.0",
